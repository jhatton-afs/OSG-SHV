            BEGIN CASE
               CASE MODE = 'DEFAULT'
                  NOT.ASK = 1
                  IF IOT='O' THEN
                     EXPENSE.CODE='1234'
                  END
                  IF IOT='I' THEN
                     EXPENSE.CODE='5678'
                  END
               CASE MODE = 'TRANSLATE' ; NULL
               CASE MODE = 'FORMAT' ; NULL
               CASE MODE = 'VERIFY' ; NULL
            END CASE
*******************************************************************************************************
         CASE CLIENT.ID='00057'          ; * Alloy Piping
            BEGIN CASE
               CASE MODE = 'DEFAULT' ; NULL
               CASE MODE = 'TRANSLATE' OR MODE = 'EDI'
                  BEGIN CASE
                     CASE ORIG.EXP='6499' ; NULL
                     CASE ORIG.EXP MATCHES '12N'
                        IF ORIG.EXP[11,2] = '50' OR ORIG.EXP[11,2] = '30' THEN
                           ERROR.MESSAGE = "Warning! Coded as a Third Party Shipment!  DO NOT PAY THIS BILL FOR ALLOY!"
                        END ELSE
                           EXPENSE.CODE=ORIG.EXP[1,4]:'-':ORIG.EXP[5,2]:'-':ORIG.EXP[7,4]:'-':ORIG.EXP[11,2]
                        END
                        IF ORIG.EXP[11,2] = '40' THEN
                           EXPENSE.CODE=ORIG.EXP[1,4]:'-':ORIG.EXP[5,2]:'-':ORIG.EXP[7,4]:'-10'
                        END
                     CASE ORIG.EXP MATCHES '10N'
