CLIENT.ID='01977'
      CALL GET.USER(USER)                ; * JMK06
      OPEN '','BCUST' TO F.CLIENT ELSE CALL OPEN.ABORT("BCUST",PROG.NAME)
      READU CLIENT.REC FROM F.CLIENT,CLIENT.ID ELSE
         RELEASE F.CLIENT,CLIENT.ID
         CALL ELINE('Client number ':CLIENT.ID:' not on file.')
         STOP
      END
      OPEN '','VEND.CUST,':CLIENT.ID TO F.VC ELSE CALL OPEN.ABORT("VEND.CUST,":CLIENT.ID,PROG.NAME)
*DF      OPEN '','DIVISION.CODES,':CLIENT.ID TO F.DIV.CODES ELSE
*DF         EXECUTE 'CREATE-FILE DATA DIVISION.CODES,':CLIENT.ID:' 7' PASSLIST CAPTURING OUTPUT
*DF         OPEN '','DIVISION.CODES,':CLIENT.ID TO F.DIV.CODES ELSE
*DF            CALL OPEN.ABORT("DIVISION.CODES",PROG.NAME)
*DF         END
*DF      END
                  EXECUTE 'SELECT VEND.CUST,':CLIENT.ID
      LOOP
         READNEXT VEND.ID ELSE VEND.ID=""
      UNTIL VEND.ID="" DO
         READ VEND.REC FROM F.VC,VEND.ID ELSE VEND.REC = ''
         IF TRIM(VEND.REC<3>) = "," THEN VEND.REC<3> = ""
                  SQLVIN=''              ; * JMK06
                  SQLVIN<1>=USER         ; * JMK06
                  SQLVIN<2>=CLIENT.ID    ; * JMK06
                  SQLVIN<3>=VEND.ID       ; * JMK06
                  SQLVOUT=''             ; * JMK06
                  CALL UPD.SQL.VC.SUB(SQLVIN,SQLVOUT,VEND.REC)   ; * JMK06
      REPEAT
STOP
