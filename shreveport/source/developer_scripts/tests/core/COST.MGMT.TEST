      SUBROUTINE(RESULT,CLIENT.REC)
      TODAY=DATE()
      LOCATE TODAY IN CLIENT.REC<84> BY 'AR' SETTING DPOS THEN NULL ELSE DPOS-=1
      IF DPOS<1 THEN DPOS=1
      CM.TEST='N'
      IF CLIENT.REC<85,DPOS>+0#0 THEN CM.TEST='Y'
      IF SUM(CLIENT.REC<210,DPOS>)+0#0 THEN CM.TEST='Y'
      IF SUM(CLIENT.REC<216,DPOS>)+0#0 THEN CM.TEST='Y'
      IF CLIENT.REC<128,DPOS>+0#0 THEN CM.TEST='Y'
      IF SUM(CLIENT.REC<209,DPOS>)+0#0 THEN CM.TEST='Y'
      IF SUM(CLIENT.REC<215,DPOS>)+0#0 THEN CM.TEST='Y'
      RESULT=CM.TEST
      RETURN
   END
