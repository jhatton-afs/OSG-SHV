               CASE ONE.COPY=0 AND SUMMARY.ONLY=0  ; * JMK137
                  DATA 'N'               ; * JMK137
               CASE ONE.COPY=1 AND SUMMARY.ONLY=0  ; * JMK137
                  DATA 'Y'               ; * JMK137
            END CASE                     ; * JMK137
            DATA ONLY.VARIANCES          ; * JMK137
            CHAIN "FB.3.1.REG"           ; * JMK137
         CASE 1
