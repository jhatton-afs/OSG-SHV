      OPEN 'STAT.FILE' TO FILE.STAT ELSE STOP 201,"GBSTAT"
      OPEN 'GBSPEC.DATA2' TO FILE.NAME ELSE STOP 201."GBSPEC.DATA"
      EOJ = 0
****      EXECUTE "SELECT STAT.FILE"
EXECUTE "GET-LIST MAIN.ACCT"
      LOOP
         READNEXT ID ELSE EOJ = 1
      UNTIL EOJ DO
         READ STAT FROM FILE.STAT,ID THEN
            NAME = STAT<1>
            READV TST FROM FILE.NAME,NAME,1 ELSE
               WRITEV "" ON FILE.NAME,NAME,1
            END
         END
      REPEAT
