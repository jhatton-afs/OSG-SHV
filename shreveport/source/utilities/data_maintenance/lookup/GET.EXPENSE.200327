                     ORIG.CO.FLAG='Y'
                  END ELSE ORIG.CO.FLAG='N'
                  LOCATE DEST.ZIP IN LOC.ZIPS SETTING POS THEN
                     DEST.CO.FLAG='Y'
                  END ELSE DEST.CO.FLAG='N'
                  BEGIN CASE
                     CASE ORIG.CO.FLAG='Y' AND DEST.CO.FLAG='Y'
                        BEGIN CASE
                           CASE IOT='I' AND ORIG.ZIP='74012' AND (DEST.ZIP='28152' OR DEST.ZIP = '28150')
