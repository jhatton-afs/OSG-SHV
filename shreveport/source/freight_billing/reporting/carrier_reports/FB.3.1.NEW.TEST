                        END ELSE
                           INS PRO.REC<9> BEFORE DIVS<1,POS>

                           INS 0 BEFORE DIVS<3,POS>
                           INS 0 BEFORE DIVS<4,POS>

                           DIVS.COST=PRO.REC<8>+PRO.REC<42>+PRO.REC<43>+PRO.REC<44>+PRO.REC<165>     ; * JMK07
                           INS DIVS.COST BEFORE DIVS<5,POS>
                           INS 0 BEFORE DIVS<6,POS>
                        END              ; * LOCATE PRO.REC<9> IN DIVS<1>,1 BY 'AL' SETTING POS
                     END                 ; * IF DIVISION='Y'
                     EDI.PPR='PPR'       ; * JMK45
                     IF PRO.REC<88>#'' OR PRO.REC<53>[1,12]='EBILL.IMPORT' THEN  ; * JMK45
                        EDI.PPR='EDI'    ; * JMK45
                     END                 ; * JMK45

                     IF EDI.PPR='EDI' THEN         ; * JMK45

                        BEGIN CASE       ; * JMK97
                           CASE CLIENT='01148' AND END.DATE GT ICONV('07/10/08','D')       ; * JMK99
                              IF CARRIER='00015' OR CARRIER='01075' THEN         ; * JMK99
                                 NUM.EDI.OUT<1,2>=NUM.EDI.OUT<1,2>+1   ; * JMK99
                                 IF NOT(SEPARATE.DIVISIONS) THEN       ; * JMK99
                                    DIV.COUNTS<8,DIV.POS>+=1           ; * JMK99
                                 END     ; * JMK99
                                 IF CARRIER='00015' THEN     ; * JMK99
                                    IF CLIENT='01148' THEN   ; * JMK99
                                       LOCATE PRO.REC<94> IN EDI.COUNT.ARRAY<1> BY 'AR' SETTING EDI.COUNT.POS ELSE       ; * JMK99
                                          INS PRO.REC<94> BEFORE EDI.COUNT.ARRAY<1,EDI.COUNT.POS>    ; * JMK99
                                       END         ; * JMK99
