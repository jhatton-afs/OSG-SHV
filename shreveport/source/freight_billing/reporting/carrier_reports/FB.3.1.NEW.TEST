                  Y.IN.CHG.DUE=DFB.BILLS.YTD<21>+DFB.BILLS.YTD<67>     ; * new field
                  Y.THIRD.CHG.DUE=DFB.BILLS.YTD<62>+DFB.BILLS.YTD<69>  ; * new field
