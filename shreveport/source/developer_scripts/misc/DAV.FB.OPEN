OPEN '','FB.OPEN.BILLS' TO F.OPEN.BILLS ELSE STOP
OPEN '','FB.BILLS.HIST,05904' TO F.HIST ELSE STOP
STMT='GET-LIST DAV.058'
EXECUTE STMT
READNXT:**
READNEXT ID ELSE STOP
READ PAY.REC FROM F.HIST,ID ELSE 
  CALL ELINE(ID:' is now missing')
  GO READNXT
END
PAY.ID='05904*':ID
WRITE PAY.REC ON F.OPEN.BILLS,PAY.ID
GO READNXT
STOP
