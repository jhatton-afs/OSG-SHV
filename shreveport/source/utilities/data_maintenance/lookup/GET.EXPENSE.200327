                           ERROR.MESSAGE = "Warning! Coded as a Third Party Shipment!  DO NOT PAY THIS BILL FOR ALLOY!"
                        END ELSE
                           EXPENSE.CODE=ORIG.EXP[1,4]:'-':ORIG.EXP[5,2]:'-':ORIG.EXP[7,2]
                        END
                        IF ORIG.EXP[7,2] = '40' THEN
                           EXPENSE.CODE=ORIG.EXP[1,4]:'-':ORIG.EXP[5,2]:'-10'
                        END
                     CASE 1
                        ERROR.MESSAGE = "Incorrect Expense Code Format!!!"
                  END CASE
