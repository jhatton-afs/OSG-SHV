               HTML<-1>='<TD COLSPAN="2" ALIGN="RIGHT">':FRT.PAID'R26,#30':'</TD>'
               HTML<-1>='<TD COLSPAN="3" ALIGN="RIGHT">':Y.FRT.PAID'R26,#40':'</TD></TR>'
