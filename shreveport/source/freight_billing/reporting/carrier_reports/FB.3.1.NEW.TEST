         COUNTER=COUNTER+1
         CALL PROG.BAR(22,COUNTER,NUM.ITMS,PER)
         READ PRO.REC FROM F.HIST,PRO.ID THEN
            LINE.DA=''
            CARRIER=FIELD(PRO.ID,'*',1)
            IF CARRIER#PREV.CARRIER AND CARRIER NE '' THEN
               IF COPIES#0 THEN PRINT
