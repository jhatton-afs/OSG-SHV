      SUBROUTINE(RESULT)
      CARRIER.ID=FIELD(@ID,'*',1)
      CLIENT.ID=@RECORD<1>
      BEGIN CASE
         CASE CARRIER.ID='00041'
            RESULT=@RECORD<7>
         CASE 1
DEBUG
            STMT='SELECT EDI.BILLS.HIST,':CLIENT.ID:' WITH CARRIER# = "':CARRIER.ID:'" WITH 132 SAMPLE 1 132'
            EXECUTE STMT CAPTURING RESULT
      END CASE
      RETURN
   END
