      OPEN 'GBT' TO FILE.CLI ELSE STOP
      OPEN 'UPSDTL,05626' TO FILE.SOURCE ELSE STOP
      FOUND = 0
      EOJ = 0
      EXECUTE "GET-LIST CHECK.125"
      LOOP
         READNEXT ID ELSE EOJ = 1
      UNTIL EOJ DO
         FOUND = 0
         READU CDAT FROM FILE.CLI,ID THEN
            WRITE CDAT ON FILE.SOURCE,ID
         END
      REPEAT
      CRT FOUND
