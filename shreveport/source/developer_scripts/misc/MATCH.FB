      SUBROUTINE MATCH.FB(RESULT,CRX.ID,CRX.REC)
      CLIENT.ID=CRX.REC<1>
      OPEN '','FB.BILLS.HIST,':CLIENT.ID TO F.HIST ELSE
         RESULT='CANNOT OPEN FILE FB.BILLS.HIST,':CLIENT.ID
         RETURN
      END
      READ HIST.REC FROM F.HIST,CRX.ID THEN
         RESULT='FOUND'
      END ELSE
         RESULT='NOT FOUND'
      END
      RETURN
      END

