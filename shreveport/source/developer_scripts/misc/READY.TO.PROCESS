      SUBROUTINE READY.TO.PROCES(RESULT,BILL.REC)
      CLIENT.ID=FIELD(@FILENAME,',',2)
      CARRIER.ID=FIELD(@ID,'*',1)
      PRO=FIELD(@ID,'*',2)
      SEQ=FIELD(@ID,'*',3)
      RTP=1
      BEGIN CASE
         CASE CLIENT.ID='01977'
            RTP=0
            IF CARRIER.ID='00015' THEN RTP=1
            IF CARRIER.ID='00041' THEN RTP=1
            IF CARRIER.ID='01075' THEN RTP=1
            IF BILL.REC<39>='O' AND TRIM(BILL.REC<241>)#'' AND TRIM(BILL.REC<242>)#'' AND TRIM(BILL.REC<243>)='' THEN RTP=1
         CASE CLIENT.ID='01979'
            RTP=0
            IF CARRIER.ID='00015' THEN RTP=1
            IF CARRIER.ID='00041' THEN RTP=1
            IF CARRIER.ID='01075' THEN RTP=1
            IF CARRIER.ID='12790' THEN RTP=1
            IF BILL.REC<39>='O' AND TRIM(BILL.REC<241>)#'' AND TRIM(BILL.REC<242>)#'' AND TRIM(BILL.REC<243>)='' THEN RTP=1
         CASE 1
            IF BILL.REC<39>='R' THEN RTP=0
            IF SEQ > 0 THEN RTP=0
      END CASE
      RESULT=RTP
      RETURN
      END
